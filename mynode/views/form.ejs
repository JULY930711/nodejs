<%- include('parts/html-head')%> <%- include('parts/html-navbar')%>

<div class="container">
  <div class="row">
    <div class="col">
      <form method="post">
        <div class="mb-3">
          <label for="account" class="form-label">Email address</label>
          <input type="text" class="form-control" id=account"
          aria-describedby="emailHelp" name="account">
        </div>
        <div><%= locals.account? account:"" %></div>
        <div class="mb-3">
          <label for="password" class="form-label">Password</label>
          <input
            type="password"
            class="form-control"
            id="password"
            name="password"
          />
        </div>
        <div><%= locals.password? password:"" %></div>

        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>
</div>

<%- include('parts/html-scripts')%> <%- include('parts/html-foot')%>
